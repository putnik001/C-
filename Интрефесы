Colleg2 colleg = new Colleg2();
colleg.Info("Иван");
colleg.Error("Ваня");

public interface Ilogger
{
    string Name { get; } 
    void Info(string student);
    void Error(string ticher);
}

class Colleg2 : Ilogger
{
    public string Name { get; } = "2П1";
    public void Info(string student)
    {
        Console.WriteLine($"{student} ,  {2 + 5} abc группа: {Name}");
    }

    public void Error(string teacher)
    {
        Console.WriteLine($"{teacher} , {3 + 3} def группа: {Name} ");
    }
    
}
